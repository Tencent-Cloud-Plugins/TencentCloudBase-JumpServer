{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "jumpserver",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "serviceName": "jumpserver",
          "servicePath": "/",
          "uploadType": "image",
          "imageInfo": {
            "imageUrl": "ccr.ccs.tencentyun.com/tcbimages/jumpserver-tcb:2.8.3"
          },
          "containerPort": 80,
          "envVariables": {
            "DB_HOST": "{{env.DB_HOST}}",
            "DB_PORT": "{{env.DB_PORT}}",
            "DB_NAME": "jumpserver",
            "DB_USER": "root",
            "DB_PASSWORD": "{{env.DB_PASSWORD}}"
          },
          "volumeMounts": {
            "/opt/jumpserver/data": "jumpserver-data",
            "/opt/koko/data": "koko-data",
            "/config/guacamole/data": "guacamole-data"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "jumpserver-data"
        },
        {
          "type": "CFS",
          "name": "koko-data"
        },
        {
          "type": "CFS",
          "name": "guacamole-data"
        },
        {
          "type": "CynosDB",
          "name": "jumpserver",
          "envMap": {
            "IP": "DB_HOST",
            "PORT": "DB_PORT",
            "USERNAME": "DB_USER",
            "PASSWORD": "DB_PASSWORD"
          }
        }
      ]
    }
  }
}